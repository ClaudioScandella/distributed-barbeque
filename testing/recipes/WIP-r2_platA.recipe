<?xml version="1.0"?>
<BarbequeRTRM version="0.1">
	<application priority="4">
		<awms>
			<awm name="wm0" value="8">
				<resources>
					<mem qty="1" units="Mb"/>
					<dma qty="50" units="Mbps"/>
					<arch name="P2012">
						<mem qty="32" units="Kb"/>
						<clusters label "Set of clusters">
							<mem qty="32" units="Kb"/>
							<cluster id="0">
								<pe qty="2">
									<mem id="0" qty="2" units="Kb"/>
								</pe>
								<mem qty="8" units="Kb"/>
								<dma qty="30" units="Mbps"/>
							</cluster>
						</clusters>
					</arch>
				</resources>
				<plugins>
					<plugin name="YaMCa" type="optimizer" required="y">
						<!-- Add here some plugin specific data requirements -->
					</plugin>
				</plugins>
			</awm>
			<awm name="wm1" value="4">
				<resources>
					<mem qty="512" units="Kb"/>
					<dma qty="20" units="Mbps"/>
					<arch name="P2012">
						<mem qty="16" units="Kb"/>
						<clusters>
							<mem qty="16" units="Kb">
							<cluster id="0">
								<pe qty="3">
									<mem qty="2" units="Kb"/>
									<mem qty="1" units="Kb"/>
								</pe>
								<mem qty="2" units="Kb"/>
								<dma qty="10" units="Mbps"/>
							</cluster>
						</clusters>
					</arch>
				</resources>
			</awm>
			<awm name="wm2" value="2">
				<resources>
					<mem qty="256" units="Kb" label="System Memory"/>
